aport_var <- function(l,lsum)
{
l/lsum
}
lsum <- sum(eig2$values)
(var2 <-  aport_var(eig2$values,lsum))
plot(var2,type="o",ylab = "Porcentaje de varianza expresada", xlab = "Componente")
n <- nrow(data_2) #nÃºmero de datos
