#create dumms
create_dums <- function(df, cols){
#create dumms
for (categ in cols){
df <- cbind(df, dummy(df[,categ], sep = '_'))
}
#delete old cols
df[,cols] <- NULL
#delete one dummie column to avoid colineality
df[,'df1_low'] <- NULL
df[,'df1_not_married'] <- NULL
df[,'df1_non_grad'] <- NULL
return(df)
}
